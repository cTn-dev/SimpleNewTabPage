{
    "manifest_version": 2,
    "minimum_chrome_version": "37",
    "version": "0.4",

    "author": "cTn",
    "name": "Simple New Tab Page",

    "chrome_url_overrides": {
        "newtab": "main.html"
    },

    "permissions": [
        "topSites",
        "management",
        "sessions",
        "tabs",
        "storage",
        "chrome://favicon/"
    ],

    "icons": {
        "128": "images/icon_128.png"
    }
}